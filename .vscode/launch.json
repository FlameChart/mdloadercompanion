{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch via NPM",
            "request": "launch",
            "runtimeArgs": ["dev"],
            "runtimeExecutable": "pnpm",
            "skipFiles": ["<node_internals>/**"],
            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run tauri in Dev",
            "runtimeExecutable": "tauri",
            "cwd": "${workspaceFolder}",
            "args": ["dev"]
        }
    ]
}
